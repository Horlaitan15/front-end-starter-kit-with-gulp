//- Line Mixin
mixin line
	if (!demo)
		="\n"

//- Tab Mixin
mixin tab
	if (!demo)
		="\t"

//- LineTab Mixin
mixin lineTab
	+line
	+tab    

//- Displaceable Image Mixin with Placeholder
mixin img(photo,alt)
	img(src=(!demo ? "assets/img/" + photo : "assets/img/sample/" + photo) alt=alt)


//- Deleted Command Line Mixin
mixin c(command)
	+line 
	if (!demo) 	
		<!-- #{command} -->
